print("CALCULATOR")
n1=int(input("Enter 1st number- "))
n2=int(input("Enter 2nd number- "))
sym=input("Enter the operation symbol- ")
if (sym=="+"):
    print(n1 + n2)
elif (sym=="-"):
    print(n1 - n2)
elif (sym=="*"):
    print(n1 *n2)
elif (sym=="/"):
    print(n1 / n2)
else :
    print("invalid operation")
